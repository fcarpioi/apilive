# Configuración para webhooks
WEBHOOK_API_KEY=your-secret-api-key-here

# Configuración de Copernico API
COPERNICO_DEV_BASE_URL=https://demo-api.copernico.cloud
COPERNICO_DEV_API_KEY=your-dev-api-key-here
COPERNICO_PROD_BASE_URL=https://vendor-api.copernico.cloud
COPERNICO_PROD_API_KEY=your-prod-api-key-here

# Configuración de comportamiento Copernico
COPERNICO_TIMEOUT_MS=10000
COPERNICO_RETRY_ATTEMPTS=3
COPERNICO_RETRY_DELAY_MS=1000
COPERNICO_CACHE_TTL_MINUTES=30
COPERNICO_ENABLE_CACHE=true
COPERNICO_LOG_REQUESTS=false
COPERNICO_LOG_RESPONSES=false

